{
  "name": "polars-shack-proxy",
  "version": "1.0.0",
  "description": "Custom NGINX-style proxy server for Polar's Shack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "docker:build": "docker build -t polars-shack-proxy .",
    "docker:run": "docker run -p 3001:3001 polars-shack-proxy"
  },
  "keywords": ["proxy", "cors", "nginx", "web-proxy", "unblock"],
  "author": "Polar's Shack",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
